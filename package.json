{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "prettier": "prettier --config .prettierrc --write \"**/*.+(js)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.9.1",
    "react-navigation-header-buttons": "^2.3.1",
    "react-redux": "6",
    "redux": "4",
    "redux-actions": "^2.6.5",
    "redux-api-middleware": "^3.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "prettier": "^1.17.0",
    "react-dom": "^16.8.6"
  },
  "eslintConfig": {
    "extends": "@wyscout/eslint-config-wyscout",
    "env": {
      "jest": true
    },
    "globals": {
      "SyntheticEvent": true,
      "SyntheticAnimationEvent": true,
      "SyntheticClipboardEvent": true,
      "SyntheticCompositionEvent": true,
      "SyntheticInputEvent": true,
      "SyntheticUIEvent": true,
      "SyntheticFocusEvent": true,
      "SyntheticKeyboardEvent": true,
      "SyntheticMouseEvent": true,
      "SyntheticDragEvent": true,
      "SyntheticWheelEvent": true,
      "SyntheticTouchEvent": true,
      "SyntheticTransitionEvent": true,
      "SyntheticProgressEvent": true
    }
  },
  "private": true,
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-tests.js"
    ],
    "preset": "react-native"
  }
}
